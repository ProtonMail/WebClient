#: src/app/templates/modals/hotkey.tpl.html:12
msgctxt "Info"
msgid "<kbd>?</kbd> Opens the <strong>help modal</strong>."
msgstr "<kbd>?</kbd> Apre la <strong>modalità aiuto</strong>."

#: src/app/templates/modals/hotkey.tpl.html:33
msgctxt "Info"
msgid ""
"<kbd>.</kbd> Marks or unmarks the message/conversation as <strong>starred</"
"strong>."
msgstr ""
"<kbd>.</kbd> Segna o non segna il messaggio/conversazione come "
"<strong>preferito</strong>."

#: src/app/templates/modals/hotkey.tpl.html:28
msgctxt "Info"
msgid ""
"<kbd>*</kbd> then <kbd>a</kbd> <strong>Select all</strong> conversations."
msgstr ""
"<kbd>*</kbd> quindi <kbd>a</kbd> <strong>Seleziona tutte</strong> le "
"coversazioni."

#: src/app/templates/modals/hotkey.tpl.html:29
msgctxt "Info"
msgid ""
"<kbd>*</kbd> then <kbd>n</kbd> <strong>Unselect all</strong> conversations."
msgstr ""
"<kbd>*</kbd> quindi <kbd>n</kbd> <strong>Deseleziona tutte</strong> le "
"conversazioni."

#: src/app/templates/modals/hotkey.tpl.html:13
msgctxt "Info"
msgid "<kbd>/</kbd> Focus the <strong>search input</strong>."
msgstr "<kbd>/</kbd> Mette a fuoco <strong>l'input di ricerca</strong>."

#: src/app/templates/modals/hotkey.tpl.html:60
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Marks the <strong>next</"
"strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd>Seleziona il "
"<strong>prossimo</strong>messaggio/conversazione."

#: src/app/templates/modals/hotkey.tpl.html:61
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Marks the last message."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Seleziona l'ultimo "
"messaggio."

#: src/app/templates/modals/hotkey.tpl.html:62
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Unmarks the message."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd>Non seleziona il "
"messaggio."

#: src/app/templates/modals/hotkey.tpl.html:59
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd> Marks the <strong>previous</"
"strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd>Seleziona il "
"<strong>precedente</strong>messaggio/conversazione."

#: src/app/templates/modals/hotkey.tpl.html:36
msgctxt "Info"
msgid ""
"<kbd>a</kbd> Moves the message/conversation to <strong>archive</strong>."
msgstr ""
"<kbd>a</kbd> Sposta il messaggio/conversazione nell'<strong>archivio</"
"strong>."

#: src/app/templates/modals/hotkey.tpl.html:19
msgctxt "Info"
msgid "<kbd>c</kbd> Opens a new <strong>composer</strong>."
msgstr "<kbd>c</kbd> Apre un nuovo<strong>compositore</strong>."

#: src/app/templates/modals/hotkey.tpl.html:21
msgctxt "Info"
msgid ""
"<kbd>command</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr ""
"<kbd>command</kbd> + <kbd>invio</kbd> <strong>Invia</strong>il messaggio."

#: src/app/templates/modals/hotkey.tpl.html:20
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr ""
"<kbd>ctrl</kbd> + <kbd>invio</kbd> <strong>Invia</strong> il messaggio."

#: src/app/templates/modals/hotkey.tpl.html:14
msgctxt "Info"
msgid "<kbd>enter</kbd> Confirms <strong>the active modal</strong>."
msgstr "<kbd>invio</kbd> Conferma<strong>la modalità attiva</strong>."

#: src/app/templates/modals/hotkey.tpl.html:65
msgctxt "Info"
msgid ""
"<kbd>enter</kbd> Opens the <strong>marked</strong> message/conversation."
msgstr ""
"<kbd>invio</kbd> Apre il messaggio/conversazione <strong>selezionato</"
"strong>."

#: src/app/templates/modals/hotkey.tpl.html:66
msgctxt "Info"
msgid "<kbd>escape</kbd> Back to the list."
msgstr "<kbd>esc</kbd> Indietro all'elenco."

#: src/app/templates/modals/hotkey.tpl.html:15
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes <strong>the active modal</strong>."
msgstr "<kbd>esc</kbd> Chiude <strong>la modalità attiva</strong>."

#: src/app/templates/modals/hotkey.tpl.html:22
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes the active <strong>composer</strong>."
msgstr "<kbd>esc</kbd> Chiude il <strong>compositore</strong> attivo."

#: src/app/templates/modals/hotkey.tpl.html:52
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>.</kbd> Goes to <strong>starred</strong>."
msgstr "<kbd>g</kbd> quindi <kbd>.</kbd> Va ai <strong>preferiti</strong>."

#: src/app/templates/modals/hotkey.tpl.html:53
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>a</kbd> Goes to <strong>archive</strong>."
msgstr "<kbd>g</kbd> quindi <kbd>a</kbd> Va all' <strong>archivio</strong>."

#: src/app/templates/modals/hotkey.tpl.html:50
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>d</kbd> Goes to <strong>drafts</strong>."
msgstr "<kbd>g</kbd> quindi <kbd>d</kbd> Va alle <strong>bozze</strong>."

#: src/app/templates/modals/hotkey.tpl.html:49
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>i</kbd> Goes to <strong>inbox</strong>."
msgstr ""
"<kbd>g</kbd> quindi <kbd>i</kbd> Va alla <strong>posta in arrivo</strong>."

#: src/app/templates/modals/hotkey.tpl.html:51
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>s</kbd> Goes to <strong>sent</strong>."
msgstr "<kbd>g</kbd> quindi <kbd>s</kbd> Va a<strong>Inviata</strong>."

#: src/app/templates/modals/hotkey.tpl.html:55
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>t</kbd> Goes to <strong>trash</strong>."
msgstr "<kbd>g</kbd> quindi <kbd>t</kbd> Va al <strong>Cestino</strong>."

#: src/app/templates/modals/hotkey.tpl.html:54
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>x</kbd> Goes to <strong>spam</strong>."
msgstr "<kbd>g</kbd> quindi <kbd>x</kbd> Va allo <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:34
msgctxt "Info"
msgid "<kbd>i</kbd> Moves the message/conversation to <strong>inbox</strong>."
msgstr ""
"<kbd>i</kbd> Sposta i messaggi/conversazione nella <strong>posta in arrivo</"
"strong>."

#: src/app/templates/modals/hotkey.tpl.html:64
msgctxt "Info"
msgid "<kbd>j</kbd> Displays the <strong>older</strong> conversation."
msgstr "<kbd>j</kbd> Visualizza le <strong>vecchie</strong> conversazioni."

#: src/app/templates/modals/hotkey.tpl.html:63
msgctxt "Info"
msgid "<kbd>k</kbd> Displays the <strong>newer</strong> conversation."
msgstr "<kbd>k</kbd> Visualizza le <strong>nuove</strong> conversazioni."

#: src/app/templates/modals/hotkey.tpl.html:31
msgctxt "Info"
msgid "<kbd>r</kbd> Marks the message/conversation as <strong>read</strong>."
msgstr ""
"<kbd>r</kbd> Segna il messaggio/conversazione come <strong>letto</strong>."

#: src/app/templates/modals/hotkey.tpl.html:37
msgctxt "Info"
msgid "<kbd>s</kbd> Moves the message/conversation to <strong>spam</strong>."
msgstr ""
"<kbd>s</kbd> Sposta il messaggio/conversazione nello <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:42
msgctxt "Info"
msgid ""
"<kbd>shift</kbd> + <kbd>a</kbd> Reply to all recipients for a message/"
"conversation."
msgstr ""
"<kbd>shift</kbd> + <kbd>a</kbd>Risponde a tutti i destinatari di un "
"messaggio/conversazione."

#: src/app/templates/modals/hotkey.tpl.html:43
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>f</kbd> Forward a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>f</kbd>Inoltra un messaggio/conversazione."

#: src/app/templates/modals/hotkey.tpl.html:41
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>r</kbd> Reply to a message/conversation."
msgstr ""
"<kbd>shift</kbd> + <kbd>r</kbd> Risponde ad un messaggio/conversazione."

#: src/app/templates/modals/hotkey.tpl.html:35
msgctxt "Info"
msgid "<kbd>t</kbd> Moves the message/conversation to <strong>trash</strong>."
msgstr ""
"<kbd>t</kbd> Sposta il messaggio/conversazione nel <strong>cestino</strong>."

#: src/app/templates/modals/hotkey.tpl.html:32
msgctxt "Info"
msgid "<kbd>u</kbd> Marks the message/conversation as <strong>unread</strong>."
msgstr ""
"<kbd>u</kbd> Segnala il messaggio/conversazione come <strong>non letto</"
"strong>."

#: src/app/templates/modals/hotkey.tpl.html:30
msgctxt "Info"
msgid "<kbd>x</kbd> Selects the message/conversation marked."
msgstr "<kbd>x</kbd> Seleziona il messaggio/conversazione come segnalato."

