#: src/app/templates/modals/hotkey.tpl.html:12
msgctxt "Info"
msgid "<kbd>?</kbd> Opens the <strong>help modal</strong>."
msgstr "<kbd>?</kbd> Deschide <strong>ajutorul contextual</strong>."

#: src/app/templates/modals/hotkey.tpl.html:35
msgctxt "Info"
msgid ""
"<kbd>.</kbd> Marks or unmarks the message/conversation as <strong>starred</"
"strong>."
msgstr ""
"<kbd>.</kbd> Marcare sau demarcare mesaje/conversații cu <strong>stea</"
"strong>."

#: src/app/templates/modals/hotkey.tpl.html:30
msgctxt "Info"
msgid ""
"<kbd>*</kbd> then <kbd>a</kbd> <strong>Select all</strong> conversations."
msgstr ""
"<kbd>*</kbd> apoi <kbd>a</kbd> <strong>Selectare toate</strong> "
"conversațiile."

#: src/app/templates/modals/hotkey.tpl.html:31
msgctxt "Info"
msgid ""
"<kbd>*</kbd> then <kbd>n</kbd> <strong>Unselect all</strong> conversations."
msgstr ""
"<kbd>*</kbd> apoi <kbd>n</kbd> <strong>Deselectare toate</strong> "
"conversațiile."

#: src/app/templates/modals/hotkey.tpl.html:13
msgctxt "Info"
msgid "<kbd>/</kbd> Focus the <strong>search input</strong>."
msgstr "<kbd>/</kbd> Focalizare <strong>casetă căutare</strong>."

#: src/app/templates/modals/hotkey.tpl.html:62
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Marks the <strong>next</"
"strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Marcare "
"<strong>următorul</strong> mesaj/conversație."

#: src/app/templates/modals/hotkey.tpl.html:63
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Marks the last message."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Marcare ultimul mesaj."

#: src/app/templates/modals/hotkey.tpl.html:64
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Unmarks the message."
msgstr "<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Demarcare mesaj."

#: src/app/templates/modals/hotkey.tpl.html:61
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd> Marks the <strong>previous</"
"strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd> Marcare mesaj/conversație "
"<strong>anterioară</strong>."

#: src/app/templates/modals/hotkey.tpl.html:38
msgctxt "Info"
msgid ""
"<kbd>a</kbd> Moves the message/conversation to <strong>archive</strong>."
msgstr "<kbd>a</kbd> Mutare mesaj/conversație în <strong>arhivă</strong>."

#: src/app/templates/modals/hotkey.tpl.html:19
msgctxt "Info"
msgid "<kbd>c</kbd> Opens a new <strong>composer</strong>."
msgstr "<kbd>c</kbd> <strong>Compunere</strong> mesaj nou."

#: src/app/templates/modals/hotkey.tpl.html:21
msgctxt "Info"
msgid ""
"<kbd>command</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr "<kbd>cmd</kbd> + <kbd>enter</kbd> <strong>Trimite</strong> mesajul."

#: src/app/templates/modals/hotkey.tpl.html:23
msgctxt "Info"
msgid "<kbd>command</kbd> + <kbd>s</kbd> <strong>Saves</strong> the message."
msgstr "<kbd>cmd</kbd> + <kbd>s</kbd> <strong>Salvează</strong> mesajul."

#: src/app/templates/modals/hotkey.tpl.html:20
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Trimite</strong> mesajul."

#: src/app/templates/modals/hotkey.tpl.html:22
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>s</kbd> <strong>Saves</strong> the message."
msgstr "<kbd>ctrl</kbd> + <kbd>s</kbd> <strong>Salvează</strong> mesajul."

#: src/app/templates/modals/hotkey.tpl.html:14
msgctxt "Info"
msgid "<kbd>enter</kbd> Confirms <strong>the active modal</strong>."
msgstr "<kbd>enter</kbd> Confirmă <strong>fereastra modală activă.</strong>."

#: src/app/templates/modals/hotkey.tpl.html:67
msgctxt "Info"
msgid ""
"<kbd>enter</kbd> Opens the <strong>marked</strong> message/conversation."
msgstr ""
"<kbd>enter</kbd> Deschide mesajul/conversația <strong>marcată</strong>."

#: src/app/templates/modals/hotkey.tpl.html:68
msgctxt "Info"
msgid "<kbd>escape</kbd> Back to the list."
msgstr "<kbd>escape</kbd> Înapoi la listă."

#: src/app/templates/modals/hotkey.tpl.html:15
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes <strong>the active modal</strong>."
msgstr "<kbd>escape</kbd> Închide <strong>fereastra modală activă.</strong>."

#: src/app/templates/modals/hotkey.tpl.html:24
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes the active <strong>composer</strong>."
msgstr ""
"<kbd>escape</kbd> Închide fereastra de <strong>compunere</strong> activă."

#: src/app/templates/modals/hotkey.tpl.html:54
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>.</kbd> Goes to <strong>starred</strong>."
msgstr "<kbd>g</kbd> apoi <kbd>.</kbd> Merge la <strong>marcate</strong>."

#: src/app/templates/modals/hotkey.tpl.html:55
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>a</kbd> Goes to <strong>archive</strong>."
msgstr "<kbd>g</kbd> apoi <kbd>a</kbd> Merge la <strong>arhivă</strong>."

#: src/app/templates/modals/hotkey.tpl.html:52
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>d</kbd> Goes to <strong>drafts</strong>."
msgstr "<kbd>g</kbd> apoi <kbd>d</kbd> Merge la <strong>ciorne</strong>."

#: src/app/templates/modals/hotkey.tpl.html:51
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>i</kbd> Goes to <strong>inbox</strong>."
msgstr "<kbd>g</kbd> apoi <kbd>i</kbd> Merge la <strong>mesaje</strong>."

#: src/app/templates/modals/hotkey.tpl.html:53
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>s</kbd> Goes to <strong>sent</strong>."
msgstr "<kbd>g</kbd> apoi <kbd>s</kbd> Merge la <strong>trimise</strong>."

#: src/app/templates/modals/hotkey.tpl.html:57
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>t</kbd> Goes to <strong>trash</strong>."
msgstr "<kbd>g</kbd> apoi <kbd>t</kbd> Merge la <strong>gunoi</strong>."

#: src/app/templates/modals/hotkey.tpl.html:56
msgctxt "Info"
msgid "<kbd>g</kbd> then <kbd>x</kbd> Goes to <strong>spam</strong>."
msgstr "<kbd>g</kbd> apoi <kbd>x</kbd> Merge la <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:36
msgctxt "Info"
msgid "<kbd>i</kbd> Moves the message/conversation to <strong>inbox</strong>."
msgstr "<kbd>i</kbd> Mută mesajul/conversația în <strong>mesaje</strong>."

#: src/app/templates/modals/hotkey.tpl.html:66
msgctxt "Info"
msgid "<kbd>j</kbd> Displays the <strong>older</strong> conversation."
msgstr "<kbd>j</kbd> Afișează conversațiile mai <strong>vechi</strong>."

#: src/app/templates/modals/hotkey.tpl.html:65
msgctxt "Info"
msgid "<kbd>k</kbd> Displays the <strong>newer</strong> conversation."
msgstr "<kbd>k</kbd> Afișează conversațiile mai <strong>noi</strong>."

#: src/app/templates/modals/hotkey.tpl.html:33
msgctxt "Info"
msgid "<kbd>r</kbd> Marks the message/conversation as <strong>read</strong>."
msgstr "<kbd>r</kbd> Marchează mesajul/conversația ca <strong>citit</strong>."

#: src/app/templates/modals/hotkey.tpl.html:39
msgctxt "Info"
msgid "<kbd>s</kbd> Moves the message/conversation to <strong>spam</strong>."
msgstr "<kbd>s</kbd> Mută mesajul/conversația în <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:44
msgctxt "Info"
msgid ""
"<kbd>shift</kbd> + <kbd>a</kbd> Reply to all recipients for a message/"
"conversation."
msgstr ""
"<kbd>shift</kbd> + <kbd>a</kbd> Răspunde tuturor destinatarilor pentru un "
"mesaj/conversație."

#: src/app/templates/modals/hotkey.tpl.html:45
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>f</kbd> Forward a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>f</kbd> Redirecționează un mesaj/conversație."

#: src/app/templates/modals/hotkey.tpl.html:43
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>r</kbd> Reply to a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>r</kbd> Răspunde la un mesaj/conversație."

#: src/app/templates/modals/hotkey.tpl.html:37
msgctxt "Info"
msgid "<kbd>t</kbd> Moves the message/conversation to <strong>trash</strong>."
msgstr "<kbd>t</kbd> Mută mesaj/conversație în <strong>gunoi</strong>."

#: src/app/templates/modals/hotkey.tpl.html:34
msgctxt "Info"
msgid "<kbd>u</kbd> Marks the message/conversation as <strong>unread</strong>."
msgstr ""
"<kbd>u</kbd> Marchează mesajul/conversația ca <strong>necitit</strong>."

#: src/app/templates/modals/hotkey.tpl.html:32
msgctxt "Info"
msgid "<kbd>x</kbd> Selects the message/conversation marked."
msgstr "<kbd>x</kbd> Selectează mesajul/conversația marcată."

#: src/app/templates/user/humanVerification.tpl.html:57
msgctxt "About human verification"
msgid ""
"<p>Too many ProtonMail accounts have been created from your connection.</p>\n"
"                <p>Thus, we are requesting additional verification to ensure "
"you are human and not a spam bot.</p>\n"
"                <p>Because Tor is frequently abused by spammers, this check "
"may be triggered because of the Tor exit node you are using.</p>"
msgstr ""
"<p>S-au creat prea multe conturi ProtonMail de la această conexiune</p>\n"
"<p>De aceea, solicităm o verificare suplimentară pentru a ne asigura că "
"sunteți o persoană și nu un robot de spam</p>\n"
"<p>Deoarece rețeaua Tor este frecvent abuzată de către cei ce trimit spam, "
"această verificare poate fi declanșată datorită nodului Tor de ieșire pe "
"care îl folosiți</p>"

