#: src/app/templates/modals/hotkey.tpl.html:13
msgctxt "Info"
msgid "<kbd>?</kbd> Opens the <strong>help modal</strong>."
msgstr "<kbd>?</kbd> Ouvre la modal <strong>d'aide</strong>."

#: src/app/templates/modals/hotkey.tpl.html:32
msgctxt "Info"
msgid ""
"<kbd>*</kbd> Marks the message/conversation as <strong>starred</strong>."
msgstr ""
"<kbd>*</kbd> Marque le message / conversation comme <strong>suivi.</strong>"

#: src/app/templates/modals/hotkey.tpl.html:29
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Display the <strong>next</"
"strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-down\"></i></kbd> Afficher le message / "
"conversation <strong>suivant</strong>."

#: src/app/templates/modals/hotkey.tpl.html:26
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Display the "
"<strong>previous</strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Afficher le message / "
"conversation <strong>précédent</strong>."

#: src/app/templates/modals/hotkey.tpl.html:27
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Display the "
"<strong>next</strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-right\"></i></kbd> Afficher le message / "
"conversation <strong>suivant</strong>."

#: src/app/templates/modals/hotkey.tpl.html:28
msgctxt "Info"
msgid ""
"<kbd><i class=\"fa fa-long-arrow-up\"></i></kbd> Display the "
"<strong>previous</strong> message/conversation."
msgstr ""
"<kbd><i class=\"fa fa-long-arrow-left\"></i></kbd> Afficher le message / "
"conversation <strong>précédent</strong>."

#: src/app/templates/modals/hotkey.tpl.html:34
msgctxt "Info"
msgid ""
"<kbd>a</kbd> Moves the message/conversation to <strong>archive</strong>."
msgstr ""
"<kbd>a</kbd> Déplace le message / conversation dans <strong>archive</strong>."

#: src/app/templates/modals/hotkey.tpl.html:12
msgctxt "Info"
msgid "<kbd>c</kbd> Opens a new <strong>composer</strong>."
msgstr "<kbd>c</kbd> Ouvre un nouvel <strong>éditeur</strong>."

#: src/app/templates/modals/hotkey.tpl.html:19
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>d</kbd> <strong>Discards</strong> the message."
msgstr "<kbd>ctrl</kbd> + <kbd>d</kbd> <strong>Annule</strong> le message."

#: src/app/templates/modals/hotkey.tpl.html:20
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Sends</strong> the message."
msgstr "<kbd>ctrl</kbd> + <kbd>enter</kbd> <strong>Envoie</strong> le message."

#: src/app/templates/modals/hotkey.tpl.html:18
msgctxt "Info"
msgid "<kbd>ctrl</kbd> + <kbd>s</kbd> <strong>Saves</strong> the message."
msgstr "<kbd>ctrl</kbd> + <kbd>s</kbd> <strong>Enregistre</strong> le message."

#: src/app/templates/modals/hotkey.tpl.html:14
msgctxt "Info"
msgid "<kbd>escape</kbd> Closes <strong>the active modal</strong>."
msgstr "<kbd>escape</kbd> Ferme la modal <strong>active</strong>."

#: src/app/templates/modals/hotkey.tpl.html:30
msgctxt "Info"
msgid "<kbd>r</kbd> Marks the message/conversation as <strong>read</strong>."
msgstr ""
"<kbd>r</kbd> Marks le message / conversation comme <strong>lu</strong>."

#: src/app/templates/modals/hotkey.tpl.html:35
msgctxt "Info"
msgid "<kbd>s</kbd> Moves the message/conversation to <strong>spam</strong>."
msgstr ""
"<kbd>s</kbd> Déplace le message / conversation dans <strong>spam</strong>."

#: src/app/templates/modals/hotkey.tpl.html:40
msgctxt "Info"
msgid ""
"<kbd>shift</kbd> + <kbd>a</kbd> Reply to all recipients for a message/"
"conversation."
msgstr ""
"<kbd>shift</kbd> + <kbd>a</kbd> permet de répondre à tous les destinataires "
"d'un message / conversation."

#: src/app/templates/modals/hotkey.tpl.html:41
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>f</kbd> Forward a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>f</kbd> Transférer un message / conversation."

#: src/app/templates/modals/hotkey.tpl.html:39
msgctxt "Info"
msgid "<kbd>shift</kbd> + <kbd>r</kbd> Reply to a message/conversation."
msgstr "<kbd>shift</kbd> + <kbd>r</kbd> Répondre à un message / conversation."

#: src/app/templates/modals/hotkey.tpl.html:33
msgctxt "Info"
msgid "<kbd>t</kbd> Moves the message/conversation to <strong>trash</strong>."
msgstr ""
"<kbd>t</kbd> Déplace le message / conversation dans la <strong>corbeille.</"
"strong>"

#: src/app/templates/modals/hotkey.tpl.html:31
msgctxt "Info"
msgid "<kbd>u</kbd> Marks the message/conversation as <strong>unread</strong>."
msgstr ""
"<kbd>u</kbd> marque le message / conversation comme <strong>non lu</strong>."

